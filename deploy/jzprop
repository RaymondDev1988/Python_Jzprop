server {
    listen 80;
    server_name jzprop.tk www.jzprop.tk;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /root/jzprop;
    }
    location /media/ {
        root /root/jzprop;
    }
    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}